{
    "manifest_version": 3,
    "name": "Custom Quote New Tab",
    "description": "Display a custom quote on a new tab.",
    "version": "1.0",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [
        "storage",
        "search",
        "history",
        "webNavigation"
    ],
    "host_permissions": [
      "https://doa508wm14jjw.cloudfront.net/*"
    ],
    "background": {
      "service_worker": "js/background.js",
      "type": "module"
    }
  }
  