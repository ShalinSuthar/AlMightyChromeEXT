{
    "manifest_version": 3,
    "name": "Newest NewTab",
    "description": "Infuse Your New Tab with Infinite possibilities, Every Time!",
    "version": "1.1",
    "chrome_url_overrides": {
        "newtab": "newtab.min.html"
    },
    "icons": {
    "16": "static/icon16.png",
    "32": "static/icon32.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png"
  },
    "permissions": [
        "storage",
        "search"
    ],
    "background": {
        "service_worker": "bundle.min.js"
    }
}
