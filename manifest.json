{
    "manifest_version": 3,
    "name": "Custom Quote New Tab",
    "description": "Display a custom quote on a new tab.",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "newtab.min.html"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "bundle.min.js"
    }
}
